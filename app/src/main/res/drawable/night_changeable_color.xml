<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:layout_width="wrap_content" android:layout_height="wrap_content" android:shape="oval">
            <solid android:color="#B145C5" />
            <stroke android:width="1.0dip" android:color="#9A353535" />
            <corners android:radius="10.0dip" />
        </shape>
    </item>

    <item>
        <shape android:layout_width="wrap_content" android:layout_height="wrap_content" android:shape="oval">
            <solid android:color="#9A302F2F" />
            <stroke android:width="1.0dip" android:color="#9AE8E6E6" />
            <corners android:radius="10.0dip" />
        </shape>
    </item>

</selector>