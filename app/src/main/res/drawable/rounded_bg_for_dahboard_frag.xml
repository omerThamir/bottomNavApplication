<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:layout_width="match_parent" android:layout_height="wrap_content">
            <solid android:color="#F389AD"></solid>
            <stroke android:width="1.0dip" android:color="#2C2528" />
            <corners android:radius="10dp"></corners>

        </shape>
    </item>


    <item>
        <shape android:layout_width="match_parent" android:layout_height="wrap_content">
            <solid android:color="#F2E91E63"></solid>

            <stroke android:width="1.0dip" android:color="#141622" />
            <corners android:radius="10dp"></corners>

        </shape>
    </item>

</selector>
